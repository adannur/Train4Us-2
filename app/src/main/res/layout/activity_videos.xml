<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FDE6A0"
    tools:context=".VideosActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"

            android:fontFamily="@font/muli_bold"
            android:textColor="@color/Black"
            android:text="Videos">

        </TextView>
    </LinearLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

                <TextView
                    android:layout_marginTop="20sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="28sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="-Stretches-">


                </TextView>



            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <VideoView
                    android:id="@+id/catpose"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"

                    >

                </VideoView>


                </FrameLayout>



                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Cat Pose">

                </TextView>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <VideoView
                    android:id="@+id/bridge_pose"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"

                    >

                </VideoView>

                </FrameLayout>

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Bridge Stretch">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <VideoView
                    android:id="@+id/triangular_pose"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"

                    >

                </VideoView>


                </FrameLayout>
                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Triangular Pose">

                </TextView>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                <VideoView
                    android:id="@+id/downward_dog"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"

                    >

                </VideoView>

                </FrameLayout>

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Downward Dog">

                </TextView>
            </LinearLayout>



            <TextView
                android:layout_marginTop="20sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textSize="28sp"
                android:layout_gravity="center"
                android:fontFamily="@font/muli_bold"
                android:textColor="@color/Black"
                android:text="-Exercises-">

            </TextView>

            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <VideoView
                        android:id="@+id/indoor_footbals"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"

                        >

                    </VideoView>

                </FrameLayout>

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Indoor Footballs">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <VideoView
                        android:id="@+id/Catch"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"

                        >

                    </VideoView>

                </FrameLayout>

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Catch">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <VideoView
                        android:id="@+id/dancing"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"

                        >

                    </VideoView>

                </FrameLayout>

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Dancing">

                </TextView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:padding="5dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"

                android:orientation="vertical">

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <VideoView
                        android:id="@+id/indoor_hits"
                        android:layout_width="match_parent"
                        android:layout_height="200dp"

                        >

                    </VideoView>

                </FrameLayout>

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp"


                    android:textSize="20sp"
                    android:layout_gravity="center"
                    android:fontFamily="@font/muli_bold"
                    android:textColor="@color/Black"
                    android:text="Indoor Hits">

                </TextView>
            </LinearLayout>





        </LinearLayout>





    </ScrollView>
</LinearLayout>